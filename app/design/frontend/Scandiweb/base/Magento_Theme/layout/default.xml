        <referenceContainer name="header-wrapper">
        <container name="footerContainer" htmlTag="div" htmlId="cart-container-id" htmlClass="cart-container" before="-">

            <block ifconfig="carriers/freeshipping/active" class="Magento\Framework\View\Element\Template" name="customblock" before="-" template="Magento_Theme::custom.phtml" />
